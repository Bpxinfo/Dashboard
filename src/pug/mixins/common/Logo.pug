mixin Logo(config)
  - var config = Object.assign({logo:'bpxlogo', text: true, width: 100}, config)
  .d-flex.align-items-center&attributes(attributes)
    img(src=`${CWD}assets/img/icons/${config.logo}.svg` alt='phoenix' width=config.width)
    if config.text
      h5.logo-text.ms-2(class=config.textClass ? config.textClass : '') 
      //phoenix